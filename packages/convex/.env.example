# Convex Deployment (from Convex dashboard)
CONVEX_DEPLOYMENT=dev:your-deployment-here
CONVEX_URL=https://your-deployment.convex.cloud

# Clerk Authentication (backend keys for Convex functions)
CLERK_SECRET_KEY=sk_test_your_secret_key_here
